	doi = {10.5281/zenodo.6644009},
	keywords = {Attribute aggregation, Authentication and authorization infrastructure (AAI), CILogon, Data sharing, ELIXIR AAI, Federated identity management, Human Genomics, Identity and Access Management (IAM), Management actions, Security \& privacy, User policy},
